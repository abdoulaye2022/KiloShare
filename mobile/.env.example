# Configuration de l'environnement mobile KiloShare
# Copiez ce fichier vers .env et remplissez avec vos vraies valeurs

# URL de l'API backend
API_URL=http://127.0.0.1:8000/api/v1

# Configuration Cloudinary (OBLIGATOIRE)
CLOUDINARY_CLOUD_NAME=your_cloud_name
CLOUDINARY_API_KEY=your_api_key
CLOUDINARY_API_SECRET=your_api_secret
CLOUDINARY_UPLOAD_PRESET=kiloshare_uploads

# Configuration Firebase
FIREBASE_API_KEY=your_firebase_api_key
FIREBASE_PROJECT_ID=kiloshare-8f7fa
FIREBASE_SENDER_ID=450200476606
FIREBASE_APP_ID=your_firebase_app_id

# Configuration Stripe
STRIPE_PUBLISHABLE_KEY=pk_test_your_publishable_key

# Configuration Google Sign-In
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret

# Mode debug (true/false)
DEBUG=true